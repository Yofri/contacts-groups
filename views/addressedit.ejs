<!DOCTYPE html>
<% console.log(rows) %>
<html>
  <head>
    <meta charset="utf-8">
    <title>Edit Address</title>
  </head>
  <body>
    <table>
      <tr>
      <form class="" action="/addresses/edit/ <%= rows.id %>" method="post">
