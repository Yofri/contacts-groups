<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>My Contacts App</title>
  </head>
  <body>
    <h1>Contacts Page</h1>

    <table>
      <thead>
        <tr>
          <th><a href="/">Home</a></th>
          <th><a href="/contacts">Contacts</a></th>
          <th><a href="/groups">Groups</a></th>
          <th><a href="/profiles">Profile</a></th>
          <th><a href="/addresses">Addresses</a></th>
        </tr>
      </thead>
    </table><br>

    <table border="1px">
      <thead>
        <tr>
          <th>ID</th>
          <th>Name</th>
          <th>Company</th>
          <th>Telp</th>
          <th>Email</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% dataContacts.forEach((data) => { %>
          <tr>
            <td><%= data.id %></td>
            <td><%= data.name %></td>
            <td><%= data.company %></td>
            <td><%= data.telp_number %></td>
            <td><%= data.email %></td>
            <td>
              <a href="/contacts/edit/<%= data.id %>">Edit</a>
              <a href="/contacts/delete/<%= data.id %>">Delete</a>
            </td>
          </tr>
        <% }) %>
      </tbody>
    </table>

    <h4><a href="/contacts/add">Add Contact</a>&nbsp;&nbsp;<a href="/addresses_with_contact">Addresses With Contact</a></h4>
  </body>
</html>
