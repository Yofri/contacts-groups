
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Profile</title>
  </head>
  <body>

    <h1>Profile</h1>
    <a href="/">Home</a>
    <div class="body">
      <table>
        <form class="" action="/Profile" method="post">
          <input type="text" name="Username" placeholder="Username">
          <input type="password" name="password" placeholder="password">

          <input type="submit" name="" value="SUBMIT">
        </form>
        <br>
        <br>

        <tr>
          <th>id</th>
          <th>User name</th>
          <th></th>
        </tr>
        <tbody>
          <%data.forEach( row => { %>
            <tr>
              <td> <%= row.id %> </td>
              <td> <%= row.Username %></td>
              <td>
                <a href="/profile/edit/<%= row.id%>">Edit</a>
                <a href="#">Delete</a>
              </td>
            </tr>
            <% }) %>
        </tbody>
      </table>
    </div>
  </body>
</html>
