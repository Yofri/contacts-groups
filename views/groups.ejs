<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Contacts</title>
  </head>
  
  <body>
    <table>
      <thead>
        <tr>
          <td>ID</td>
          <td>Name</td>
        </tr>
      </thead>
      <tbody>
        <%groupsData.forEach( (group) => { %>
          <tr>
            <td> <%= group.ID %> </td>
            <td> <%= group.name_of_group%> </td>
            <td> <a href="/contacts/edit/<%=group.ID%>">EDID</a></td>
            <td> <a href="/contacts/delete/<%=group.ID%>">DELED</a></td>
          </tr>
          <%})%>
      </tbody>
    </table>
  </body>
</html>